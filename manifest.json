{
  "manifest_version": 2,
  "name": "Mythril",
  "description": "Security analysis tool for Ethereum smart contracts",
  "version": "0.8.5",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [{
    "run_at" :"document_end",
    "matches": ["<all_urls>"],
    "js": ["jquery-3.3.1.min.js","content.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
